{
  "builds": [
    {
      "src": "TravellerWebsite/wsgi.py",
      "use": "@vercel/python"
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": { "distDir": "staticfiles" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "TravellerWebsite/wsgi.py"
    },
    {
      "src": "/staticfiles/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    }
  ]
}
