{
  "builds": [
    {
      "src": "TravellerWebsite/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "sh build_files.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "TravellerWebsite/wsgi.py"
    }
  ]
}
