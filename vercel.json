{
  "builds": [{
      "src": "TravellerWebsite/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
  }],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/path0/staticfiles/$1"
    },
      {
          "src": "/(.*)",
          "dest": "TravellerWebsite/wsgi.py"
      }
  ]
}
